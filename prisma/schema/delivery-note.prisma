model DeliveryNote {
  id                  String               @id @default(dbgenerated("gen_random_uuid()")) @map("id") @db.Uuid
  poReceiptId         String?              @map("po_receipt_id") @db.Uuid
  warehouseStaffId    String?              @map("warehouse_staff_id") @db.Uuid
  createdAt           DateTime?            @map("created_at") @db.Timestamptz(6)
  updatedAt           DateTime?            @map("updated_at") @db.Timestamptz(6)
  deletedAt           DateTime?            @map("deleted_at") @db.Timestamptz(6)
  poReceipt           PoReceipt?           @relation(fields: [poReceiptId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  warehouseStaff      WarehouseStaff?      @relation(fields: [warehouseStaffId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  deliveryNoteDetails DeliveryNoteDetail[]
  importRequests      ImportRequest[]
  inspectionRequests  InspectionRequest[]

  @@map("delivery_note")
}
