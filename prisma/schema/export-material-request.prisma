model export_material_request {
  id                       String                          @id @db.Uuid
  production_department_id String?                         @db.Uuid
  status                   export_material_request_status?
  expected_deliver_date    DateTime?                       @db.Date
  deliver_date             DateTime?                       @db.Date
  reserve_percent_id       String?                         @db.Uuid
  create_at                DateTime?                       @db.Timestamptz(6)
  update_at                DateTime?                       @db.Timestamptz(6)
  deleted_at               DateTime?                       @db.Timestamptz(6)
  production_department    production_department?          @relation(fields: [production_department_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  reserve_percent          reserve_percent?                @relation(fields: [reserve_percent_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

enum export_material_request_status {
  PENDING
  REJECTED
  APPROVED
  IN_PROGRESS
  FINISHED
}
