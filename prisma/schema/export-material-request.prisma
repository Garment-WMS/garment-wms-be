model ExportMaterialRequest {
  id                     String                       @id @default(dbgenerated("gen_random_uuid()")) @map("id") @db.Uuid
  productionDepartmentId String?                      @db.Uuid
  reservePercentId       String?                      @db.Uuid
  status                 ExportMaterialRequestStatus?
  expectedDeliverDate    DateTime?                    @db.Date
  deliverDate            DateTime?                    @db.Date
  createdAt              DateTime?                    @db.Timestamptz(6)
  updatedAt              DateTime?                    @db.Timestamptz(6)
  deletedAt              DateTime?                    @db.Timestamptz(6)
  productionDepartment   ProductionDepartment?        @relation(fields: [productionDepartmentId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  reservePercent         ReservePercent?              @relation(fields: [reservePercentId], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@map("export_material_request")
}

enum ExportMaterialRequestStatus {
  PENDING
  REJECTED
  APPROVED
  IN_PROGRESS
  FINISHED

  @@map("export_material_request_status")
}
