/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model product_formula {
  id          String    @id @db.Uuid
  product_id  String?   @db.Uuid
  material_id String?   @db.Uuid
  quantity    Float?
  create_at   DateTime? @db.Timestamptz(6)
  update_at   DateTime? @db.Timestamptz(6)
  deleted_at  DateTime? @db.Timestamptz(6)
  material    material? @relation(fields: [material_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  product     product?  @relation(fields: [product_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
