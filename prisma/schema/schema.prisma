generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearch", "postgresqlExtensions", "prismaSchemaFolder"]
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  directUrl  = env("DIRECT_URL")
  extensions = [pg_stat_statements, pgsodium, supabase_vault, uuid_ossp(map: "uuid-ossp", schema: "extensions")]
}
