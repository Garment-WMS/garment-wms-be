model PoDeliveryDetail {
  id                      String      @id @default(dbgenerated("gen_random_uuid()")) @map("id") @db.Uuid
  poDeliveryId            String?     @map("po_delivery_id") @db.Uuid
  materialId              String?     @map("material_id") @db.Uuid
  productId               String?     @map("product_id") @db.Uuid
  totalAmount             Float?      @map("total_ammount")
  expireDate              DateTime?   @map("expire_date") @db.Timestamptz(6)
  quantityByPackagingUnit Float?      @map("quantity_by_packaging")
  quantityByUom           Float?      @map("quantity_by_uom")
  createdAt               DateTime?   @map("created_at") @db.Timestamptz(6)
  updatedAt               DateTime?   @updatedAt @map("updated_at") @db.Timestamptz(6)
  deletedAt               DateTime?   @map("deleted_at") @db.Timestamptz(6)
  material                Material?   @relation(fields: [materialId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  poDelivery              PoDelivery? @relation(fields: [poDeliveryId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  product                 Product?    @relation(fields: [productId], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@map("po_delivery_detail")
}
