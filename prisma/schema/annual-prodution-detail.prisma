model annual_production_detail {
  id                        String                  @id @db.Uuid
  annual_production_plan_id String?                 @db.Uuid
  product_id                String?                 @db.Uuid
  demand_quantity           Float?
  starting_quantity         Float?
  quantity_to_produce       Float?
  note                      String?                 @db.VarChar
  start_date                DateTime?               @db.Date
  finish_date               DateTime?               @db.Date
  create_at                 DateTime?               @db.Timestamptz(6)
  update_at                 DateTime?               @db.Timestamptz(6)
  deleted_at                DateTime?               @db.Timestamptz(6)
  annual_production_plan    annual_production_plan? @relation(fields: [annual_production_plan_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  product                   product?                @relation(fields: [product_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
