model production_patch_order {
  id                             String                         @id @db.Uuid
  quarterly_production_detail_id String?                        @db.Uuid
  quantity_to_produce            Float?
  expected_start_date            DateTime?                      @db.Date
  expected_finish_date           DateTime?                      @db.Date
  status                         production_patch_order_status?
  create_at                      DateTime?                      @db.Timestamptz(6)
  update_at                      DateTime?                      @db.Timestamptz(6)
  deleted_at                     DateTime?                      @db.Timestamptz(6)
  quarterly_production_detail    quarterly_production_detail?   @relation(fields: [quarterly_production_detail_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

enum production_patch_order_status {
  IN_PROGRESS
  CANCELLED
  FINISHED
}
