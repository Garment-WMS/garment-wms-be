model import_request {
  id                    String                  @id @db.Uuid
  delivery_note_id      String?                 @db.Uuid
  warehouse_staff_id    String?                 @db.Uuid
  status                import_request_status?
  from                  String?                 @db.VarChar
  to                    String?                 @db.VarChar
  start_at              DateTime?               @db.Timestamptz(6)
  finish_at             DateTime?               @db.Timestamptz(6)
  create_at             DateTime?               @db.Timestamptz(6)
  update_at             DateTime?               @db.Timestamptz(6)
  deleted_at            DateTime?               @db.Timestamptz(6)
  delivery_note         delivery_note?          @relation(fields: [delivery_note_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  warehouse_staff       warehouse_staff?        @relation(fields: [warehouse_staff_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  import_request_detail import_request_detail[]
}

enum import_request_status {
  PENDING
  REJECTED
  APPROVED
  IN_PROGRESS
  FINISHED
}
