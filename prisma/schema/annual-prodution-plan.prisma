model annual_production_plan {
  id                        String                      @id @db.Uuid
  factory_director_id       String?                     @db.Uuid
  year                      Int?
  code                      String?                     @unique @db.VarChar
  status                    production_status?
  expected_start_date       DateTime?                   @db.Date
  expected_end_date         DateTime?                   @db.Date
  start_date                DateTime?                   @db.Date
  finish_date               DateTime?                   @db.Date
  create_at                 DateTime?                   @db.Timestamptz(6)
  update_at                 DateTime?                   @db.Timestamptz(6)
  deleted_at                DateTime?                   @db.Timestamptz(6)
  annual_production_detail  annual_production_detail[]
  factory_director          factory_director?           @relation(fields: [factory_director_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  quarterly_production_plan quarterly_production_plan[]
}
