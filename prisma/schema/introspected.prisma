model production_patch_order {
  id                             String                        @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  quarterly_production_detail_id String?                       @db.Uuid
  quantity_to_produce            Float
  expected_start_date            DateTime                      @db.Date
  expected_finish_date           DateTime                      @db.Date
  status                         production_patch_order_status @default(IN_PROGRESS)
  created_at                     DateTime?                     @default(now()) @db.Timestamptz(6)
  updated_at                     DateTime?                     @db.Timestamptz(6)
  deleted_at                     DateTime?                     @db.Timestamptz(6)
  quarterly_production_detail    QuarterlyProductionDetail?    @relation(fields: [quarterly_production_detail_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model reserve_percent {
  id                          String                      @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  percent                     Float
  created_at                  DateTime?                   @default(now()) @db.Timestamptz(6)
  updated_at                  DateTime?                   @db.Timestamptz(6)
  deleted_at                  DateTime?                   @db.Timestamptz(6)
  export_material_request     ExportMaterialRequest[]
  quarterly_production_detail QuarterlyProductionDetail[]
}

enum production_patch_order_status {
  IN_PROGRESS
  CANCELLED
  FINISHED
}
